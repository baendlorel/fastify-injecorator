{
  "name": "fastify-injecorator",
  "version": "0.0.1",
  "description": "A NestJS like fastify plugin for dependency injection",
  "description_zh": "一个类似于 NestJS 的 Fastify 依赖注入插件",
  "purpose": "npm",
  "type": "module",
  "exports": {
    "import": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "lines": "clear && find ./src -type f \\( -name \"*.ts\" -o -name \"*.tsx\" -o -name \"*.js\" -o -name \"*.mjs\" -o -name \"*.cjs\" -o -name \"*.cpp\" -o -name \"*.h\" -o -name \"*.py\"  -o -name \"*.css\" -o -name \"*.rs\" -o -name \"*.rc\" \\) | xargs wc -l",
    "testall": "clear && vitest",
    "stress": "clear && vitest '.stress.e2e-spec.ts'",
    "e2e": "clear && vitest '.e2e-spec.ts'",
    "spec": "clear && vitest '.spec.ts'",
    "test": "clear && vitest '.test.ts'",
    "build": "tsx ./.scripts/rollup.ts",
    "check": "tsc --noEmit",
    "lint": "oxlint .",
    "dev": "tsx watch src/main.ts"
  },
  "author": {
    "name": "Kasukabe Tsumugi",
    "email": "futami16237@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/baendlorel/fastify-injecorator"
  },
  "keywords": [
    "nestjs",
    "decorator",
    "fastify",
    "typescript",
    "javascript"
  ],
  "license": "MIT",
  "dependencies": {
    "@fastify/autoload": "^6.0.0",
    "@fastify/one-line-logger": "^2.0.2",
    "@fastify/sensible": "^6.0.0",
    "@fastify/static": "^8.2.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "axios": "^1.10.0",
    "concat-arr": "^1.3.0",
    "fastify": "^5.4.0",
    "fastify-type-provider-zod": "4.0.2",
    "mysql2": "^3.14.1",
    "reflect-deep": "^1.3.0",
    "rxjs": "^7.8.2",
    "serial-task": "^1.1.0",
    "to-assigned": "^1.1.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@anatine/zod-mock": "^3.14.0",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/preset-env": "^7.28.3",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "oxlint": "^1.12.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.46.3",
    "rollup-plugin-const-enum": "^1.1.4",
    "rollup-plugin-dts": "^6.2.3",
    "rollup-plugin-func-macro": "^1.1.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}